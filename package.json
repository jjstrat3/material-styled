{
  "name": "material-styled",
  "version": "1.0.0",
  "description": "Material themed React components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "build:flow": "flow-copy-source -v -i src lib",
    "clean": "nwb clean-module && npm clean-demo",
    "flow": "flow check",
    "lint": "eslint src",
    "start": "nwb serve-react-demo",
    "pretest": "npm run lint",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "bugs": {
    "url": "https://github.com/jjstrat3/material-styled/issues"
  },
  "dependencies": {
    "polished": "^0.0.0",
    "styled-components": "^1.4.3"
  },
  "peerDependencies": {
    "react": "^0.14 || ~15.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.38.0",
    "flow-copy-source": "^1.1.0",
    "nwb": "0.15.x",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.0.4"
  },
  "author": {
    "name": "Jay Stratton",
    "url": "https://github.com/jjstrat3"
  },
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjstrat3/material-styled.git"
  },
  "keywords": [
    "react-component",
    "styled-components"
  ]
}
